<?xml version="1.0" encoding="utf-8"?>
<Project>

  <!-- Import the higher level common properties -->
  <Import Project="..\Directory.Build.props" />

  <PropertyGroup>
    <CodeAnalysisRuleSet>$(MSBuildThisFileDirectory)\..\.sonarlint\sonaranalyzer-dotnetCSharp.ruleset</CodeAnalysisRuleSet>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>

  <ItemGroup>
    <AdditionalFiles Include="$(MSBuildThisFileDirectory)\..\.sonarlint\SonarLint.xml" Link="Properties\SonarLint.xml" />
  </ItemGroup>

</Project>